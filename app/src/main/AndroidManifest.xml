<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taxtelecom.arinamurasheva.addressbook">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ContactListApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Contact.View.ContactActivity"></activity>
        <activity android:name=".Authenticator.View.AuthenticatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ContactList.View.ContactListActivity"></activity>
        <activity android:name=".Authenticator.SharedPreferencesManager"></activity>
    </application>

</manifest>